{
  "manifest_version": 3,
  "name": "Billable",
  "version": "1.0.0",
  "description": "Track billable hours for lawyers",
  "permissions": [
    "storage",
    "alarms"
  ],
  "host_permissions": [
    "https://*.supabase.co/*"
  ],
  "action": {
    "default_popup": "public/popup.html"
  },
  "background": {
    "service_worker": "dist/background.js"
  }
}
